[user]
	email = theodor.rene@gmail.com
	name = Theodor Ren√© Carlsen
[gui]
	encoding = utf-8
[init]
	defaultBranch = main
[core]
	quotepath = off
	autocrlf = input
[alias]
	co = checkout
	st = status
	c = commit -a
    am = commit --ammend --no-edit
[filesystem "N/A|14.0.1|tmpfs"]
	timestampResolution = 9000 nanoseconds
	minRacyThreshold = 12860 microseconds
[pull]
	rebase = true
[rebase]
	autoStash = true
[merge]
	tool = nvimdiff
