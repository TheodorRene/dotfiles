[user]
	email = theodor.rene@gmail.com
	name = Theodor Ren√© Carlsen
[gui]
	encoding = utf-8
[init]
	defaultBranch = main
[core]
	quotepath = off
	autocrlf = input
[alias]
	co = checkout
	st = status
	c = commit -a
    amend = commit --amend --no-edit
    p = push
    pl = pull
    pf = push --force-with-lease
    prfix = !git commit -a --amend --no-edit && git push --force-with-lease
[filesystem "N/A|14.0.1|tmpfs"]
	timestampResolution = 9000 nanoseconds
	minRacyThreshold = 12860 microseconds
[pull]
	rebase = true
[rebase]
	autoStash = true
[merge]
	tool = nvimdiff
